  "devDependencies": {
    "@shopify/prettier-plugin-liquid": "^0.4.0",
    "@types/scrollbooster": "^2.2.2",
    "autoprefixer": "^10.4.12",
    "fast-glob": "^3.2.12",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "vite": "^3.1.8"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.3",
    "@splidejs/splide-extension-grid": "^0.4.1",
    "iconify-icon": "^1.0.1",
    "scrollbooster": "^3.0.2"
