{
  "name": "kiera-2022",
  "version": "1.0.0",
  "description": "Kiera Shopify theme 2022",
  "main": "index.js",
  "scripts": {
    "dev": "vite build --watch --minify false",
    "build": "vite build",
    "theme": "shopify theme serve --theme-editor-sync",
