{
  "name": "kiera-2022",
  "version": "1.0.0",
  "description": "Kiera Shopify theme 2022",
  "main": "index.js",
  "scripts": {
    "dev": "vite build --watch --minify false",
    "build": "vite build",
    "theme": "SHOPIFY_FLAG_STORE=elite-infinite-elements.myshopify.com shopify theme dev --theme-editor-sync --theme elite-ie-theme-dev",
    "push": "SHOPIFY_FLAG_STORE=elite-infinite-elements.myshopify.com shopify theme push",
    "pull": "SHOPIFY_FLAG_STORE=elite-infinite-elements.myshopify.com shopify theme pull",
    "dawn:update": "git pull upstream main",
    "dawn:fetch": "git fetch upstream",
    "dawn:upstream": "git remote add upstream https://github.com/Shopify/dawn.git"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shopify/prettier-plugin-liquid": "^0.4.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/scrollbooster": "^2.2.2",
    "autoprefixer": "^10.4.14",
    "fast-glob": "^3.2.12",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "vite": "^3.2.5"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.4",
    "@splidejs/splide-extension-grid": "^0.4.1",
    "iconify-icon": "^1.0.7",
    "scrollbooster": "^3.0.2"
  }
}