    "39e64e18-3f0e-4873-8098-f5fd22dcb8f0": {
      "type": "rich-text-2",
      "blocks": {
        "template--16689943773466__39e64e18-3f0e-4873-8098-f5fd22dcb8f0-1666336147034b9027-1": {
          "type": "text",
          "settings": {
            "text": "<p><em>We only have one Earth. And we believe everyone on it is precious.<br\/>We create jewelry to be a permanent part of our lives for generations, all the while challenging ourselves to keep improving our teams, our suppliers, our customers, the communities, and the environment we operate within.<\/em><\/p>"
          }
        },
