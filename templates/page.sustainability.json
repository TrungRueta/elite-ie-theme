    "8087d3bb-d94f-4768-b5c5-e93d453e23e3": {
      "type": "image-banner-2",
      "blocks": {
        "template--16689943773466__8087d3bb-d94f-4768-b5c5-e93d453e23e3-16663360046ce4f539-0": {
          "type": "heading",
          "settings": {
            "heading": "Our Sustainability",
            "heading_size": "h0",
            "heading_style": "2"
          }
        },
        "template--16689943773466__8087d3bb-d94f-4768-b5c5-e93d453e23e3-16663360046ce4f539-1": {
          "type": "text",
          "settings": {
            "text": "We make jewelry with a conscience.",
            "text_style": "body"
          }
        },
        "a6e217fa-8145-488d-add5-38eaa4fb5472": {
          "type": "text",
          "settings": {
            "text": "When Kiera was founded in 2005, we were focused on sustainability and commited to conflict-free materials, reducing the environmental impact while also seeking transparency and fair pricing along the supply chain.",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "template--16689943773466__8087d3bb-d94f-4768-b5c5-e93d453e23e3-16663360046ce4f539-0",
